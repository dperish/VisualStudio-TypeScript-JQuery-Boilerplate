{
  "compileOnSave": true,
  "compilerOptions": {
    "noImplicitAny": true,
    "noEmitOnError": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es5",
    "module": "amd",
    "moduleResolution": "classic",
    "outDir": "scripts/",
    "baseUrl": ".",
    "paths": {
      "*": [
        "*",
        "scripts/*",
        "scripts/jquery/*"
      ]
    }
  },
  "include": [
    "scripts/typings/**/*.ts",
    "app/**/*"
  ],
  "exclude": [
  ]
}